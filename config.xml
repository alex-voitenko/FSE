<?xml version='1.0' encoding='utf-8'?>
<widget id="renelfse.v210.id" version="2.1.0" versionCode="210" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/mobione/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>RenelFSE</name>
    <description>This Template is derived from RenelAppFSE V0101. 
It is used as Template for generating RenelFSE0110 (Version 1.1).
The following features have been added:
- GoogleMap improvement (Routes)
- All Lists are in alphabethical order</description>
    <author email="info@renelco.com" href="http://renelco.com">
		Renelco SA
	</author>
    <content src="RenelcoBMA_TimeClock.html" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3500" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <gap:plugin name="com.badrit.emailcomposer" />
    <gap:plugin name="com.genuitec.mobione.phoneui" />
    <gap:plugin name="com.phonegap.plugins.childbrowser" />
    <icon src="www/branding/icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/branding/icon/android/icon-36-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/branding/icon/android/icon-48-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/branding/icon/android/icon-72-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/branding/icon/android/icon-96-xhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/branding/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/branding/screen/android/screen-mdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/branding/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/branding/screen/android/screen-xhdpi-portrait.png" />
    <access browserOnly="true" origin="*" subdomains="true" />
    <access browserOnly="true" origin="http://google.com" subdomains="true" />
    <access browserOnly="true" origin="https://google.com" subdomains="true" />
    <access browserOnly="true" origin="http://maps.google.com" subdomains="true" />
    <access browserOnly="true" origin="http://*.google.com" subdomains="true" />
    <access browserOnly="true" origin="content:///*" subdomains="true" />
    <gap:platform name="android" />
    <gap:config-file mode="merge" parent="/manifest" platform="android" xmlns:android="http://schemas.android.com/apk/res/android">
        <application android:debuggable="true" />
    </gap:config-file>
    <gap:config-file mode="add" parent="/manifest" platform="android" xmlns:android="http://schemas.android.com/apk/res/android">
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION">
        </uses-permission>
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION">
        </uses-permission>
        <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS">
        </uses-permission>
        <uses-permission android:name="android.permission.BATTERY_STATS">
        </uses-permission>
        <uses-permission android:name="android.permission.CALL_PHONE" />
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-permission android:name="android.permission.INTERNET" />
        <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS">
        </uses-permission>
        <uses-permission android:name="android.permission.READ_PHONE_STATE">
        </uses-permission>
        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE">
        </uses-permission>
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">
        </uses-permission>
        <uses-permission android:name="android.permission.VIBRATE" />
    </gap:config-file>
    <gap:splash src="www/branding/splash.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="www/branding/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="www/branding/screen/android/screen-mdpi-portrait.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="www/branding/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="www/branding/screen/android/screen-xhdpi-portrait.png" />
    <gap:plugin name="org.apache.cordova.console" source="plugins.cordova.io" version="0.2.13">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.device" source="plugins.cordova.io" version="0.3.0">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.camera" source="plugins.cordova.io" version="0.3.6">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.battery-status" source="plugins.cordova.io" version="0.2.12">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.device-motion" source="plugins.cordova.io" version="0.2.11">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.device-orientation" source="plugins.cordova.io" version="0.3.11">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.file" source="plugins.cordova.io" version="1.3.3">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.file-transfer" source="plugins.cordova.io" version="0.5.0">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.geolocation" source="plugins.cordova.io" version="0.3.12">
    </gap:plugin>
    <gap:config-file mode="merge" parent="NSLocationAlwaysUsageDescription" platform="ios">
        <array>
            <string>Allow GPS</string>
        </array>
    </gap:config-file>
    <gap:plugin name="org.apache.cordova.media" source="plugins.cordova.io" version="0.2.16">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.media-capture" source="plugins.cordova.io" version="0.3.6">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.network-information" source="plugins.cordova.io" version="0.2.15">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.splashscreen" source="plugins.cordova.io" version="1.0.0">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.statusbar" source="plugins.cordova.io" version="0.1.10">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.vibration" source="plugins.cordova.io" version="0.3.13">
    </gap:plugin>
    <gap:plugin name="io.litehelpers.cordova.sqlite" source="plugins.cordova.io" version="0.7.7">
    </gap:plugin>
    <gap:plugin name="org.apache.cordova.dialogs" source="plugins.cordova.io" version="0.3.0">
    </gap:plugin>
    <gap:plugin name="com.ionic.keyboard" source="plugins.cordova.io" version="1.0.4">
    </gap:plugin>
    <preference name="splash-screen-duration" value="3500" />
    <preference name="windowSoftInputMode" value="adjustNothing" />
    <gap:plugin name="com.plugin.appinfo" source="plugins.cordova.io" version="0.0.1">
    </gap:plugin>
    <preference name="loadUrlTimeoutValue" value="600000" />
    <gap:plugin name="com.plugin.infobox" source="plugins.cordova.io" version="0.0.1">
    </gap:plugin>
    <engine name="android" spec="^6.2.3" />
</widget>
